import csv

# 1. మనం ఎంటర్ చేయాల్సిన డేటా (Data Entry List)
data = [
    ["Name", "Task", "Status"],
    ["User1", "Data Entry", "Completed"],
    ["User2", "Python Coding", "Pending"],
    ["User3", "Forensics", "Started"]
]

# 2. మొబైల్‌లో ఒక CSV ఫైల్ క్రియేట్ చేయడం
file_name = "daily_work.csv"

try:
    with open(file_name, mode='w', newline='') as file:
        writer = csv.writer(file)
        writer.writerows(data)
    print(f"సక్సెస్! '{file_name}' ఫైల్ క్రియేట్ అయ్యింది.")
except Exception as e:
    print(f"Error: {e}")
